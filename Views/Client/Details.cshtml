@model Atelier_2.Models.Client

@{
    ViewData["Title"] = "Client Details";
}

<h1>@ViewData["Title"]</h1>

<div class="card">
    <div class="card-body">
        <h5 class="card-title">Client Information</h5>
        <p><strong>First Name:</strong> @Model.ClientFirstName</p>
